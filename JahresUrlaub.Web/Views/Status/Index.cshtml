@* Hier wurde alle Anträge für Geschäftsführer angezeigt *@
@model IEnumerable<JahresUrlaub.Web.Models.Events>


    @* Hier die List wo alle Daten von Urlaub der Mitarbeitern wie Start-EndDatum gezeigt *@
    <table class="table-condensed">
        <tr>
            <th>
                Vorname und Personalnummer
            </th>
           
            <th>
                @Html.DisplayNameFor(model => model.Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.End)
            </th>
            <th>
                Status
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Subject)
                </td>
            
                <td>
                    @Html.DisplayFor(modelItem => item.Start)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.End)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ThemeColor)
                </td>
            </tr>
        }

    </table>
 
